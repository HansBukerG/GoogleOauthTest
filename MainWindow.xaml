<Window x:Class="GoogleTest02.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoogleTest02"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="400">
            <Button Name="googleSignInButton" Click="GoogleSignInButton_Click" Content="Iniciar Sesión con Google" Margin="5" Height="40"/>
            <Button Name="googleSignOutButton" Click="GoogleSignOutButton_Click" Content="Cerrar Sesión de Google" Margin="5" Height="40"/>
        </StackPanel>
    </Grid>
</Window>
